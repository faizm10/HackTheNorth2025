# backend/.env.example
# Copy this file to .env and fill in your actual values

# AI/LLM API Keys
GEMINI_API_KEY=your_gemini_api_key_here
MARTIAN_API_KEY=your_martian_api_key_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_URL=your_supabase_url_here

# Routing Configuration
ROUTING_MODE=balanced        # quality|balanced|cheap
AB_SHADOW_PERCENT=0.1        # 0..1
ROUTING_MAX_LATENCY_MS=4500  # global default

# Databricks Configuration (Optional)
DATABRICKS_HOST=your_databricks_host_here
DATABRICKS_HTTP_PATH=your_databricks_http_path_here
DATABRICKS_TOKEN=your_databricks_token_here
